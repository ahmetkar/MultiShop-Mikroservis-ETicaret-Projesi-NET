
<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AdminLayoutHeadComponentPartial")

<body class="sidebar-mini">
    <div class="wrapper">

        @await Component.InvokeAsync("_AdminLayoutHeaderComponentPartial")

      
        @await Component.InvokeAsync("_AdminLayoutSidebarComponentPartial")



        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @await Component.InvokeAsync("_AdminLayoutTopSectionComponentPartial")

            <!-- Main content -->
            @RenderBody()
            <!-- content -->
        </div>
        <!-- content-wrapper -->
        <!-- Main Footer -->
        <footer class="main-footer dark-bg">
            <div class="pull-right hidden-xs"> Version 1.0</div>
            Copyright &copy; 2017 Yourdomian. All rights reserved.
        </footer>
    </div>
    <!-- wrapper -->
    <!-- jQuery -->
    <script src="~/ovioadmin/dist/js/jquery.min.js"></script>
    <script src="~/ovioadmin/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/ovioadmin/dist/js/ovio.js"></script>
</body>
</html>